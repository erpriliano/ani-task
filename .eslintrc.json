{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "react-app", // "eslint:recommended
        "standard-with-typescript",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        },
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react",
        "@typescript-eslint",
        "prettier"
    ],
    "ignorePatterns": [
        "src/react-app-env.d.ts",
        "src/setupTests.ts",
        "src/reportWebVitals.ts"
    ],
    "rules": {
    }
}
